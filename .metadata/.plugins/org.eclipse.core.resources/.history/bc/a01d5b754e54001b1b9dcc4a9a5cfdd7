package com.revature.day01;

import java.util.Date;

public class Student{
	private int id;
	private String name;
	private Date dob;
	
	public void inputData(int id,String name){
		this.id=id;
		this.name=name;
		this.dob=new Date();
	}

	public void dispalyData(){
		System.out.println("ID : "+id+" , Name : "+name+" , DOB : "+dob);
	}