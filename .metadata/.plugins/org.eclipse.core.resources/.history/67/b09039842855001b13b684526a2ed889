package customExceptions;

public class ExtendedArrayException extends ArrayIndexOutOfBoundsException {

	private static final long serialVersionUID = 1L; //IDE suggested that I add this, not sure what it does
	static String str;
	
	ExtendedArrayException(String s){
		super(s);
		str = s;
	}
	
	public String getMessage(){
		return str;
	}
}
